{
    "manifest_version": 2,
    "name": "MD-Newtab",
    "description": "MD-themed new tab page that looks pretty. :)",
    "version": "0.1",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
	"permissions": ["bookmarks"],
	"background": {
		"scripts": ["eventPage.js"],
		"persistent": false
	}
}
