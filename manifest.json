{
    "manifest_version": 2,
    "name": "MD-Newtab",
    "description": "MD-themed new tab page that looks pretty. :)",
    "version": "0.2",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
	"permissions": ["bookmarks", "storage"],
	"background": {
		"scripts": ["eventPage.js"],
		"persistent": false
	},
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    }
}
